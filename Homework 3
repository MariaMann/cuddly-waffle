// chem haskacel xntir@, kaskacum em vor chishta 
public class Main {
   public static void main(String[] args) {
       System.out.println("Input a digit to check if it prime or not ");
       Scanner scanner = new Scanner(System.in);
       int givenInt = scanner.nextInt();
       for (int i = 2; i <= givenInt / 2; i++) {
           if (givenInt % i == 0) {
               System.out.println("Digit is not prime");
           }else{
               System.out.println("Digit is prime");
           }
       }
   }
}

№12 Sxals chem gtnum es xntri mej
public class Main {
   public static void main(String[] args) {
       System.out.println("Input a number to display Fibonacci series: ");
       Scanner scanner = new Scanner(System.in);
       int firstFibonacciNumbers = scanner.nextInt();
       int firstTerm = 0, secondTerm = 1;
       System.out.println("Here is the Fibonacci series upto " + firstFibonacciNumbers + "terms");

       for (int i = 1; i <= firstFibonacciNumbers; i++) {
           System.out.print(firstTerm + " " + secondTerm);
           int sum = firstTerm + secondTerm;
           firstTerm = secondTerm;
           secondTerm = sum;
       }
   }
}

№13
public class Main {
   public static void main(String[] args) {
      String stringToReverse = "ROBOTS_WILL_KILL_ALL_HUMANZ";
      String reversedString = "";
      for (int i = stringToReverse.length() - 1; i >= 0; i--) { //harc ka
          reversedString = reversedString +stringToReverse.charAt(i);
      }
      System.out.println("Reversed string is: ");
      System.out.println(reversedString);
   }
}

№14
public class Main {
   public static void main(String[] args) {
      Random random = new Random();
      int[] arrayOFRandomElemnets = new int[10];
      int sum = 0;
      for (int i = 0; i < arrayOFRandomElemnets.length; i++) {
          arrayOFRandomElemnets[i] = random.nextInt();
          sum += arrayOFRandomElemnets[i];
      }
      System.out.println("Array of random elements: " + sum );
   }
}

